<process name="CBPEventProcessEngine"
               targetNamespace="http://xmlns.oracle.com/EmployeeApp/CBPDemoApplication/CBPEventProcessEngine"
               xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
               xmlns:client="http://xmlns.oracle.com/EmployeeApp/CBPDemoApplication/CBPEventProcessEngine"
               xmlns:ora="http://schemas.oracle.com/xpath/extension"
               xmlns:ui="http://xmlns.oracle.com/soa/designer"
               xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/jms/EmployeeApp/CBPDemoApplication/CBPEventSubscriber"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension">

  <import ui:processWSDL="true" namespace="http://xmlns.oracle.com/EmployeeApp/CBPDemoApplication/CBPEventProcessEngine" location="../WSDLs/CBPEventProcessEngine.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      PARTNERLINKS                                                      
      List of services participating in this BPEL process               
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  --> 
  <partnerLinks>
    <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
    <partnerLink name="cbpeventprocessengine_client" partnerLinkType="client:CBPEventProcessEngine" myRole="CBPEventProcessEngineProvider"/>
    <partnerLink name="CBPEventSubscriber" partnerLinkType="ns1:Consume_Message_plt" myRole="Consume_Message_role"/>
  </partnerLinks>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
      VARIABLES                                                        
      List of messages and XML documents used within this BPEL process 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <variables>
    <!-- Reference to the message passed as input during initiation -->
    <variable name="inputVariable" messageType="ns1:Consume_Message_msg"/>

  </variables>

  <!-- 
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
     ORCHESTRATION LOGIC                                               
     Set of activities coordinating the flow of messages across the    
     services integrated within this business process                  
    ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
  -->
  <sequence name="main">

    <!-- Receive input from requestor. (Note: This maps to operation defined in CBPEventProcessEngine.wsdl) -->
    <receive name="receiveInput" partnerLink="CBPEventSubscriber" portType="ns1:Consume_Message_ptt"
             operation="Consume_Message" variable="inputVariable" createInstance="yes"/>

  </sequence>
</process>